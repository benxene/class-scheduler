  "bugs": {
    "url": "https://github.com/benxene/class-scheduler/issues"
  },
  "homepage": "https://github.com/benxene/class-scheduler#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.23",
    "babel-jest": "^27.0.5",
    "husky": "^7.0.1",
    "jest": "^27.0.5",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
